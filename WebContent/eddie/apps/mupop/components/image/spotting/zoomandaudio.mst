<img id="zoomandaudio_image" src="{{url}}" />
<div id="zoomandaudio_layers">
	{{#nodes}}
	<img id="zoomandaudio_layer{{id}}" class="zoomandaudio_spotmask" src="{{mask}}" />
	{{/nodes}}
</div>

<div id="zoomandaudio_holder">
	<div id="zoomandaudio_spot"></div>
</div>
<audio id="zoomandaudio_audio" controls="controls" autoplay="autoplay">
	<source type="audio/mpeg"/>
</audio>